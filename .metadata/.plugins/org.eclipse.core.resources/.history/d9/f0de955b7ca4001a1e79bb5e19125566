public interface SungjukMapper {
	ArrayList<SungjukVO> getSungjuklist();
	SungjukVO selectSungjuk(SungjukVO member);
	int insertSungjuk(SungjukVO member);
	int updateSungjuk(SungjukVO member);
	int deleteSungjuk(SungjukVO sungjukVO);
}
