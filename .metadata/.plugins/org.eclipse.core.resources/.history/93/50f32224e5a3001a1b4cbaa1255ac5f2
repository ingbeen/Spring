package com.spring.mapper;

import java.util.ArrayList;

import com.spring.sungjuk2.SungjukVO;


public interface SungjukMapper {
	ArrayList<SungjukVO> getMembers();
	// ArrayList<MemberVO> getMembers(String t);
	// MemberVO getMember(String id);
	HashMap<String, String> getMember(String id); // hashMap 이용시 추가부분
	// 삽입 후 삽입한 결과 상태 반환하기 위해 반환값을 int로 정해줌
	int insertMember(SungjukVO member);
	void updateMember(SungjukVO member);
	void deleteMember(String id);
	int getCount();
}
